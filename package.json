{
	"name": "vscode-uppercasesql",
	"displayName": "Upper Case SQL",
	"publisher": "eplak",
	"description": "makes all SQL keywords upper case with an easy hotkey and includes som short and easy snippets",
	"repository": {
		"url": "https://github.com/eplak/vscode-uppercasesql"
	},
	"version": "2.1.0",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:sql"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
			"command": "extension.SQLuppercase",
			"title": "SQL Uppercase"
			}
		],
		"keybindings": [
			{
				"command": "extension.SQLuppercase",
				"key": "ctrl+alt+s"
			}
		],
		"snippets": [
			{
				"language": "sql",
				"path": "./snippets/sql.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
